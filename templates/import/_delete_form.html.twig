<form method="post" action="{{ path('import_delete', {'id': import.id}) }}" onsubmit="return confirm('Voulez-vous supprimer cette importation ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ import.id) }}">
    <button class="btn btn-secondary text-primary">Supprimer</button>
</form>